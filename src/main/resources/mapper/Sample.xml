<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.securiry.study.dao.SampleMapper">
    <select id="getUser" resultType="KartUser">
		SELECT USER_ID
				,USER_EMAIL
				,USER_PASSWORD
				,REG_DATE
				,UPD_DATE
				,ROLE
				,LEVEL
		  FROM KART_USER
    </select>

</mapper>